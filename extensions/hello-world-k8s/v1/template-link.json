{
    "name": "[concat(EXTENSION_TARGET_VM_NAME_PREFIX, copyIndex(EXTENSION_LOOP_OFFSET), 'HelloWorldK8s')]",
    "type": "Microsoft.Resources/deployments",
    "apiVersion": "[variables('apiVersionDeployments')]",
    "dependsOn": [
        "[concat('Microsoft.Compute/virtualMachines/', EXTENSION_TARGET_VM_NAME_PREFIX, copyIndex(EXTENSION_LOOP_OFFSET), '/extensions/cse', '-EXTENSION_TARGET_VM_TYPE-', copyIndex(EXTENSION_LOOP_OFFSET))]"
        
    ],
    "copy": {
        "count": "EXTENSION_LOOP_COUNT",
        "name": "vmLoopNode"
    },
    "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri": "https://raw.githubusercontent.com/ebandis/aks-engine-extensions-testapp-v1/master/extensions/hello-world-k8s/v1/template.json",
            "contentVersion": "1.0.0.0"
        },
        "parameters": {
            "artifactsLocation1": {
                "value": "https://raw.githubusercontent.com/ebandis/aks-engine-extensions-testapp-v1/master/"
            },
            "apiVersionDeployments1": {
                "value": "[variables('apiVersionDeployments')]"
            },
            "targetVMName1": {
                "value": "[concat(EXTENSION_TARGET_VM_NAME_PREFIX, copyIndex(EXTENSION_LOOP_OFFSET))]"
            },
            "targetVMType1": {
                "value": "EXTENSION_TARGET_VM_TYPE"
            },
            
            "vmIndex1":{
                "value": "[copyIndex(EXTENSION_LOOP_OFFSET)]"
            }
        }
    }
}